    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "daisyui": "^5.1.9",
    "drizzle-kit": "^1.0.0-beta.1-c0277c0",
    "lefthook": "^1.12.4",
    "tailwindcss": "^4.1.13",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "ultracite": "5.3.4",
    "vite-tsconfig-paths": "^5.1.4",
    "wireit": "^0.14.12"
  },
  "name": "TODO",
  "packageManager": "pnpm@10.0.0",
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "prepare": "wireit",
    "start": "wireit",
    "test": "wireit",
    "test:ci": "wireit"
  },
  "type": "module",
  "wireit": {
    "build": {
      "dependencies": [
        "start:build"
      ]
    },
    "dev": {
      "dependencies": [
        "start:dev"
      ]
    },
    "prepare": {
      "command": "git rev-parse --git-dir > /dev/null 2>&1 && lefthook install || true"
    },
    "start": {
      "dependencies": [
        "start:start"
      ]
    },
    "start:build": {
      "clean": false,
      "command": "vite build",
      "output": [
        ".output/**"
      ]
    },
    "start:dev": {
      "command": "vite dev --port 3000",
      "service": true
    },
    "start:start": {
      "command": "node .output/server/index.mjs",
      "service": true
    },
    "test": {
      "dependencies": [
        "ultracite:fix",
        "typescript:check"
      ]
    },
    "test:ci": {
      "dependencies": [
        "ultracite:lint",
        "typescript:check"
      ]
    },
    "typescript:check": {
      "command": "tsc --noEmit"
    },
    "ultracite:fix": {
      "command": "ultracite fix"
    },
    "ultracite:lint": {
      "command": "ultracite lint"
    },
    "worker:dev": {
      "command": "tsx --watch src/worker/start.ts",
      "service": true
    },
    "worker:start": {
      "command": "tsx src/worker/start.ts",
      "service": true
    }
  }
}
