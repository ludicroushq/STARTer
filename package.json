{
  "name": "TODO",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "wireit",
    "dev": "wireit",
    "prepare": "husky",
    "start": "wireit",
    "test": "wireit",
    "test:ci": "wireit"
  },
  "lint-staged": {
    "*": [
      "eslint --fix",
      "prettier --ignore-unknown --write"
    ]
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.5",
    "@fontsource-variable/inter-tight": "^5.2.5",
    "@icons-pack/react-simple-icons": "^13.0.0",
    "@orpc/client": "^1.3.0",
    "@orpc/server": "^1.3.0",
    "@orpc/tanstack-query": "^0.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@t3-oss/env-core": "^0.13.6",
    "@tanstack/react-form": "^1.12.0",
    "@tanstack/react-router": "^1.120.13",
    "@tanstack/react-router-devtools": "^1.120.13",
    "@tanstack/react-start": "^1.120.13",
    "better-auth": "^1.2.8",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^1.0.0-beta.1-c0277c0",
    "lucide-react": "^0.511.0",
    "ms": "^2.1.3",
    "pg": "^8.16.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0",
    "ts-pattern": "^5.7.1",
    "vinxi": "^0.5.6",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@types/ms": "^2.1.0",
    "@types/pg": "^8.15.4",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "daisyui": "^5.0.43",
    "drizzle-kit": "^1.0.0-beta.1-c0277c0",
    "eslint": "^9.28.0",
    "eslint-plugin-package-json": "^0.33.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "neostandard": "^0.12.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wireit": "^0.14.12"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "start:build"
      ]
    },
    "dev": {
      "dependencies": [
        "start:dev"
      ]
    },
    "eslint:check": {
      "command": "eslint --max-warnings 0 ."
    },
    "eslint:fix": {
      "command": "eslint --fix --max-warnings 0 ."
    },
    "prettier:check": {
      "command": "prettier --check ."
    },
    "prettier:write": {
      "command": "prettier --write ."
    },
    "start": {
      "dependencies": [
        "start:start"
      ]
    },
    "start:build": {
      "clean": false,
      "command": "vinxi build",
      "output": [
        ".output/**"
      ]
    },
    "start:dev": {
      "command": "vinxi dev",
      "service": true
    },
    "start:start": {
      "command": "vinxi start",
      "service": true
    },
    "test": {
      "dependencies": [
        "eslint:fix",
        "prettier:write",
        "typescript:check"
      ]
    },
    "test:ci": {
      "dependencies": [
        "eslint:check",
        "prettier:check",
        "typescript:check"
      ]
    },
    "typescript:check": {
      "command": "tsc --noEmit"
    }
  }
}
