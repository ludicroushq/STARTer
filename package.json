  "packageManager": "pnpm@10.0.0",
  "wireit": {
    "build": {
      "dependencies": [
        "start:build"
      ]
    },
    "dev": {
      "dependencies": [
        "start:dev"
      ]
    },
    "eslint:check": {
      "command": "eslint --max-warnings 0 ."
    },
    "eslint:fix": {
      "command": "eslint --fix --max-warnings 0 ."
    },
    "prettier:check": {
      "command": "prettier --check ."
    },
    "prettier:write": {
      "command": "prettier --write ."
    },
    "start": {
      "dependencies": [
        "start:start"
      ]
    },
    "start:build": {
      "clean": false,
      "command": "vite build",
      "output": [
        ".output/**"
      ]
    },
    "start:dev": {
      "command": "vite dev --port 3000",
      "service": true
    },
    "start:start": {
      "command": "node .output/server/index.mjs",
      "service": true
    },
    "test": {
      "dependencies": [
        "eslint:fix",
        "prettier:write",
        "typescript:check"
      ]
    },
    "test:ci": {
      "dependencies": [
        "eslint:check",
        "prettier:check",
        "typescript:check"
      ]
    },
    "typescript:check": {
      "command": "tsc --noEmit"
    }
  }
}
